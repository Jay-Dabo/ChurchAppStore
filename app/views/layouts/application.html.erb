<!DOCTYPE html>
<html>
<head>
  <title>Churchappstore</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	<header>
		<h1 class="logo">Union at 1600</h1>
		<nav>
			<ul class="navigation">
				<li><%= link_to 'Catalog', store_path %>
				<li><%= link_to 'Product List', products_path %>
				<li><%= link_to 'Add New Product', new_product_path %>
			</ul>
		</nav>
	</header>

<%= yield %>

</body>
</html>
